<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE html>

<odoo>
    <data noupdate="0">
        <!-- ir.rules -->
        <record id="sale_simulation_line_rule_user" model="ir.access">
            <field name="name">Simulation_line sale user</field>
            <field name="model_id" ref="mrp_simulation.model_sale_simulation_line"/>
            <field name="groups" eval="[(4, ref('sales_team.group_sale_salesman'))]"/>
            <field name="perm_unlink" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_create" eval="True"/>
        </record>
        <record id="sale_missing_line_rule_user" model="ir.access">
            <field name="name">Missing_line sale user</field>
            <field name="model_id" ref="mrp_simulation.model_sale_missing_line"/>
            <field name="groups" eval="[(4, ref('sales_team.group_sale_salesman'))]"/>
            <field name="perm_unlink" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_create" eval="True"/>
        </record>
      </data>
</odoo>
